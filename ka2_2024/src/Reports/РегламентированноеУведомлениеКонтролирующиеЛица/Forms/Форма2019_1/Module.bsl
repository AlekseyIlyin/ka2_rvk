	ИначеЕсли Путь = "Файл/Документ/СвКонтрЛицМК/РаскрКонтрЛицМККосв/РаскрУчМК/СвУчПослКосв/СвИО/НомерУчПосл" И Страница.Свойство("НомерУчПослЧисло") Тогда
		Страница.НомерУчПослЧисло = ЭтотОбъект["ОТЧ"].ПривестиЗначение(Прав(СтрЗагружаемоеЗначение.ЗначениеЭлемента, 5));
	ИначеЕсли Путь = "Файл/Документ/СвКонтрЛицМК/РаскрКонтрЛицМККосв/РаскрУчМК/СвУчПослКосв/СвРосОрг/НомерУчПосл" И Страница.Свойство("НомерУчПослЧисло") Тогда
		Страница.НомерУчПослЧисло = ЭтотОбъект["ОТЧ"].ПривестиЗначение(Прав(СтрЗагружаемоеЗначение.ЗначениеЭлемента, 5));
	ИначеЕсли Путь = "Файл/Документ/СвКонтрЛицМК/РаскрКонтрЛицМККосв/РаскрУчМК/СвУчПослКосв/СведИСБезЮЛ/НомерУчПосл" И Страница.Свойство("НомерУчПослЧисло") Тогда
